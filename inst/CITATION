citHeader("To cite FGN in publications use:")

citEntry(entry = "Article",
  title        = "Hyperbolic Decay Time Series Models",
  author       = personList(
                as.person("Justin Veenstra"),
  		as.person("A. I. McLeod")
		),
  journal      = "In press",
  year         = "2012",
  volume       = " ",
  number       = " ",

  textVersion  =
  paste("Justin Veenstra, A. I. McLeod (2012).",
  "Hyperbolic Decay Time Series Models")
)

